<div class="jumbotron">
  <div class="container">
    <%= form_for [@course, @review] do |f| %>
      <div class="form-group">
        <%= f.label "Title"%>
        <%= f.text_field :title, class: "form-control"%>
      </div>
      <div class="form-group">
        <%= f.label "Rating"%> 
        <%= f.text_field :rating, class: "form-control"%>
      </div>
      <div class="form-group">
        <%= f.label "Review"%>
        <%= f.text_area :review_body, class: "form-control"%>
      </div>
      <%= f.submit class: 'btn btn-warning'%>
    <% end %>
  </div>
</div>